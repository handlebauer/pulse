# Radio Browser API Configuration
# Minimum number of votes required for a station to be included
RADIO_BROWSER_MIN_VOTES=3000

# Only include stations that are currently online
RADIO_BROWSER_ONLINE_ONLY=true

# Comma-separated list of country codes to fetch stations from
RADIO_BROWSER_COUNTRIES=US,GB,CA,AU

# Only include stations that broadcast in English
RADIO_BROWSER_ENGLISH_ONLY=true

# Base URL for the Radio Browser API
# See: https://api.radio-browser.info/ for available servers
RADIO_BROWSER_API_URL=https://de1.api.radio-browser.info/json

# Google API key for geocoding and other Google services
# Required for location-based features
GOOGLE_API_KEY=your_google_api_key_here 