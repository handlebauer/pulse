{
    "name": "radio",
    "module": "index.ts",
    "type": "module",
    "private": true,
    "scripts": {
        "prepare": "husky",
        "db:local:seed": "bun run scripts/db/seed.ts",
        "db:gen-types": "bunx supabase gen types typescript --local > src/lib/db/types.ts",
        "db:local:init": "bunx supabase db reset && bun run db:gen-types && bun run db:local:seed"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@typescript-eslint/eslint-plugin": "^8.25.0",
        "@typescript-eslint/parser": "^8.25.0",
        "eslint": "^9.21.0",
        "husky": "^9.1.7",
        "prettier": "3.5.2",
        "typescript-eslint": "^8.25.0"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "dependencies": {
        "@google/generative-ai": "^0.22.0",
        "@supabase/supabase-js": "^2.49.1",
        "dedent": "^1.5.3",
        "ora": "^8.2.0"
    }
}
