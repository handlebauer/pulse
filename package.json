{
    "name": "pulse",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write .",
        "db:local:seed": "bun run scripts/db/seed.ts",
        "db:gen-types": "bunx supabase gen types typescript --local > src/lib/db/types.ts",
        "db:local:init": "bunx supabase db reset && bun run db:gen-types && bun run db:local:seed",
        "prepare": "husky"
    },
    "dependencies": {
        "@radix-ui/react-slot": "^1.1.2",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "lucide-react": "^0.477.0",
        "next": "15.2.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "tailwind-merge": "^3.0.2",
        "tailwindcss-animate": "^1.0.7"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "eslint": "^9",
        "eslint-config-next": "15.2.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.4.3",
        "prettier": "3.5.3",
        "tailwindcss": "^4",
        "typescript": "^5"
    }
}
